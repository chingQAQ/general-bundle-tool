{
  "name": "general-bundle-tool",
  "version": "1.0.0",
  "description": "bundle tool base on rollup",
  "main": "index.js",
  "repository": "git@github.com:chingQAQ/general-bundle-tool.git",
  "author": "Achiu03 <aion.chiu@isobar.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@tsconfig/node14": "^1.0.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.4",
    "cross-env": "^7.0.3",
    "jest": "^27.2.1",
    "nodemon": "^2.0.12",
    "rollup": "^2.56.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "dev": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
    "compiler": "tsc --project ./tsconfig.json --rootDir ./src --outDir ./dist && cd ./dist && move ./index.js ../rollup.config.js",
    "test": "jest --watch '__tests__/*' --coverage",
    "build": "rollup --config rollup.config.js"
  }
}
